   "source": [
    "cnt = 0\n",
    "while ( n > 1):\n",
    "    if (n % k) == 0:\n",
    "        n = n//k\n",
    "        cnt += 1\n",
    "    else:\n",
    "        n -= 1\n",
    "        cnt += 1"
